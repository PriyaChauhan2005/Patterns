#include<bits/stdc++.h>
using namespace std;
int main(){
    //   A
    //   B C
    //   D E F
    //   G H I J 
    //   K L M N O
    int i,j,n;
    char ch;
    ch='A';
    cin>>n;
    for(i=1;i<=n;i++){
        for(j=1;j<=i;j++){
            cout<<ch<<"\t";
            ch++;
        }
        cout<<endl;
        
    }
}
