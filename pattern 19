#include<bits/stdc++.h>
using namespace std;
int main(){
    //      *
    //     *
    //    *
    //   *
    //  *
      
    int n,i,j,k;
    cin>>n;
    k=n;
    cout<<"Enter value of n:";
    cout<<endl;
    for(i=1;i<=n;i++){
        for(j=1;j<k;j++){
            cout<<" ";
        }
        cout<<"*";
        k--;
     
        cout<<endl;
    }
    return 0;
}
